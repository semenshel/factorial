#include <iostream>
using namespace std;

int main(){
    int n;
    int fact;
    cin >> n;

    if (n < 0){
        cout << "Факториал отрицательного числа не определен, ошибка!" << endl;

    }
    else if (n == 0){
        cout << "0! = 1" << endl;
    }
    else{
        for(int i = 1; i <= n; ++i){
            fact *= i;
            cout << fact << endl;
        }
    }


    return 0;
}
